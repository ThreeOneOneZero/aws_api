{
  "name": "aws-tasks-api",
  "version": "1.0.0",
  "description": "AWS Lambda + DynamoDB CRUD API para tarefas",
  "main": "dist/lambda.js",
  "scripts": {
    "build": "tsc",
    "package": "npm run build && powershell Compress-Archive -Path dist,node_modules,package.json -DestinationPath lambda.zip -Force",
    "lint": "eslint src --ext ts",
    "format": "prettier --write src"
  },
  "keywords": [
    "aws",
    "lambda",
    "dynamodb",
    "serverless"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.933.0",
    "@aws-sdk/lib-dynamodb": "^3.933.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.158",
    "@types/node": "^24.7.2",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^9.37.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3"
  }
}
